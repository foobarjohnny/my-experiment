<?xml version="1.0" encoding="UTF-8"?>
<resource_loader>	
	<!-- resource holder -->
	<holders>
		<holder name="device" type="resource_bundle"
			class="com.telenav.cserver.common.resource.device.DevicePropertiesHolder"
			config_path="device" prefix_structure="programCode,platform,version"
			floating_structure="product,device_resolution" suffix_structure="" />
			
		<holder name="messages" type="resource_bundle"
			class="com.telenav.cserver.common.resource.message.MessagesHolder"
			config_path="messages" prefix_structure="programCode,platform,version"
			floating_structure="product,device_resolution" suffix_structure="locale" />
		
		<holder name="billing_manager" type="xml"
			class="com.telenav.cserver.billing.holder.BillingManagerHolder"
			config_path="billing_manager" prefix_structure="programCode,platform,version"
			floating_structure="product,device_resolution" suffix_structure="" />
			
		<holder name="dsm" type="xml" class="com.telenav.cserver.common.resource.holder.impl.DsmRuleHolder" 		
			config_path="dsmRules" 
			prefix_structure="programCode,platform,version" 
			floating_structure="product,device_resolution"
			suffix_structure=""/>
			
		<holder name="countryMapping" type="spring" class="com.telenav.cserver.ac.holder.CountryMappingHolder" 		
			config_path="country_mapping" 
			prefix_structure="" 
			floating_structure=""
			suffix_structure=""/>
			
		<holder name="service_locator" type="spring" class="com.telenav.cserver.resource.common.ServiceLocatorHolder" 		
			config_path="serviceLocator/service_mapping" 
			prefix_structure="" 
			floating_structure="programCode,platform,version,device_resolution,product,ptn"
			suffix_structure=""/>
		
		
	</holders>
</resource_loader>
