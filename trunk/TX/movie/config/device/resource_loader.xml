<?xml version="1.0" encoding="UTF-8"?>
<resource_loader>	
	<!-- resource holder -->
	<holders>
		<holder name="device" type="resource_bundle"
			class="com.telenav.cserver.common.resource.device.DevicePropertiesHolder"
			config_path="device" prefix_structure="carrier,platform,version"
			floating_structure="product,device_resolution" suffix_structure="" />
			
		<holder name="messages" type="resource_bundle"
			class="com.telenav.cserver.common.resource.message.MessagesHolder"
			config_path="messages" prefix_structure="carrier,platform,version"
			floating_structure="product,device_resolution" suffix_structure="locale" />
		
		<holder name="dsm" type="xml" class="com.telenav.cserver.common.resource.holder.impl.DsmRuleHolder" 		
			config_path="dsmRules" 
			prefix_structure="carrier,platform,version" 
			floating_structure="product,device_resolution"
			suffix_structure=""/>
	</holders>
	
</resource_loader>
