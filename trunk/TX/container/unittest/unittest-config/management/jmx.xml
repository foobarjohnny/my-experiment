<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE beans PUBLIC "-//SPRING/DTD BEAN/EN" 
    "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<!-- this bean must not be lazily initialized if the exporting is to happen -->
	<bean id="exporter"
		class="org.springframework.jmx.export.MBeanExporter"
		lazy-init="false">
		<property name="beans">
			<map>
				<entry key="login_startup_service:name=BackendServerMonitor" value-ref="backendServerMonitor" />
			</map>
		</property>
	</bean>

	<bean name="backendServerMonitor" class="com.telenav.cserver.framework.management.jmx.BackendServerMonitorForTest1"/>
</beans>